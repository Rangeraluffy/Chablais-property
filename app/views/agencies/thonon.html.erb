            <div class="title-box-d">
              <h3 class="title-d"><%= translate('discoverThonon') %>
              </h3>
            </div>
            <p class="color-text-a">
              <%= translate('discoverTextThonon1') %>
            </p>
            <p class="color-text-a">
              <%= translate('discoverTextThonon2') %>
            </p>
            <p class="color-text-a">
              <%= translate('discoverTextThonon3') %>
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

