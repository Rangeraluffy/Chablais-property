          <%= link_to image_tag("agencies/thonon/thonon.jpg", :class => 'img-fluid')%>
        </div>
        <div class="sinse-box">
          <h3 class="sinse-title">Peillex Property - Thonon
            <br> <i class="fas fa-phone"></i> 04.50.72.62.01
          </h3>
          <p><%= translate('hours-thonon') %></p>
        </div>
      </div>
      <div class="col-md-12 section-t8 position-relative">
        <div class="row">
          <div class="col-md-6 col-lg-5">
              <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2752.970314037753!2d6.475317551182275!3d46.369976980465495!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x478c3e62389647db%3A0xa701571cacb5bf8d!2s5%20Av.%20du%20G%C3%A9n%C3%A9ral%20de%20Gaulle%2C%2074200%20Thonon-les-Bains!5e0!3m2!1sfr!2sfr!4v1633715497840!5m2!1sfr!2sfr" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
