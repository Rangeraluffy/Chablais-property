<!-- ======= Intro About ======= -->
<section class="intro-single">
  <div class="container">
    <div class="row">
      <div class="col-md-12 col-lg-8">
        <div class="title-single-box">
          <h1 class="title-single">Peillex Thonon</h1>
          <h3 class="sinse-title"">5, Avenue Général de Gaulle</h3>
        </div>
      </div>
      <div class="col-md-12 col-lg-4">
        <nav class="d-flex justify-content-lg-end">
            <%= link_to image_tag("logo_2.png", class: "logo_signIn", alt: "logo_Peillex_Property")%>
        </nav>
      </div>
    </div>
  </div>
</section>
